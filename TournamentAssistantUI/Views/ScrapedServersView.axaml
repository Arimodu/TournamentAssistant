<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="TournamentAssistantUI.Views.ScrapedServersView">

  <Panel>
    <PathIcon IsVisible="{Binding IsPasswordProtected}" Height="25" Width="25" Data="{StaticResource lock_regular}" Margin="250 0 0 0" VerticalAlignment="Center" Opacity="0.3"/>
    <StackPanel HorizontalAlignment="Left">
      <TextBlock Text="{Binding Name}" HorizontalAlignment="Left"/>
      <TextBlock Text="{Binding Address}" HorizontalAlignment="Left" Opacity="0.3"/>
    </StackPanel>
  </Panel>
</UserControl>
