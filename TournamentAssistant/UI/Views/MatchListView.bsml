<bg size-delta-y='0'>
  <horizontal bg='panel-top' pad-left='10' pad-right='10' horizontal-fit='PreferredSize' anchor-max-y='0.95' anchor-min-y='0.95' anchor-max-x='-0.1' anchor-min-x='-0.1'>
    <text text='Ongoing Matches' align='Left'></text>
  </horizontal>
  <page-button event-click='list#PageUp' direction='Up' anchor-max-y='0.95' anchor-min-y='0.95'></page-button>
  <list id='list' select-cell='match-click' expand-cell='true' list-style='Simple' ></list>
  <page-button event-click='list#PageDown' direction='Down' anchor-min-y='0.05'></page-button>
</bg>